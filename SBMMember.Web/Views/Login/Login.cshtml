@model LoginViewModel
@{
    Layout = null;
}

<!DOCTYPE html>

<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Samata Bhratru Mandal</title>
    <link rel="icon" href="~/img/favicon.png">
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/css/bootstrap.min.css">
    <!-- animate CSS -->
    <link rel="stylesheet" href="~/css/animate.css">
    <!-- owl carousel CSS -->
    <link rel="stylesheet" href="~/css/owl.carousel.min.css">
    <!-- themify CSS -->
    <link rel="stylesheet" href="~/css/themify-icons.css">
    <!-- flaticon CSS -->
    <link rel="stylesheet" href="~/css/flaticon.css">
    <!-- font awesome CSS -->
    <link rel="stylesheet" href="~/css/magnific-popup.css">
    <!-- swiper CSS -->
    <link rel="stylesheet" href="~/css/slick.css">
    <!-- style CSS -->
    <link rel="stylesheet" href="~/css/style.css">
</head>

<body>
    <!--::header part start::-->
    <header class="main_menu single_page_menu">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-12">
                    <nav class="navbar navbar-expand-lg navbar-light">
                        <a class="navbar-brand logo_1" href="#"><h2> Login </h2></a>
                        <a class="navbar-brand logo_2" href="#"><h2> Login </h2></a>
                    </nav>
                </div>
            </div>
        </div>
    </header>
    <!-- Header part end by sidd-->
    <!-- breadcrumb start-->
    <section class="breadcrumb breadcrumb_bg">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="marqu" style="height:150px"><p>***** scrolling Marquee tag line here *****</p></div>
                </div>
            </div>
        </div>
    </section>
    <!-- breadcrumb start-->
    <!--================Blog Area =================-->
    <section class="blog_area single-post-area section_padding">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 posts-list">
                    <div class="single-post">
                        <div class="blog_details">
                            <p class="excert">
                                Access with your registered mobile
                            </p>
                            <div class="login-icon">
                                <img src="~/img/login-icon.png" width="110px" height="110px">
                            </div>
                        </div>
                    </div>




                    <div class="login-type">
                        <div class="form-group">

                            <div class="container">
                                <form asp-action="Login" asp-controller="Login" method="post" enctype="multipart/form-data">
                                    <div class="row">
                                        <div class="col-lg-6 col-md-6 mt-sm-50">
                                            <input type="text" name="MobileNumber" asp-for="MobileNumber" placeholder="Mobile No." onfocus="this.placeholder = ''" onblur="this.placeholder = 'Mobile No.'"
                                                   required class="single-input-primary">
                                        </div>
                                        <div class="col-lg-6 col-md-6 mt-sm-50">
                                            <input type="text" name="MPIN" asp-for="MPIN" placeholder="6 Digit Code" onfocus="this.placeholder = ''" onblur="this.placeholder = '6 Digit Code'"
                                                   required class="single-input-primary">

                                        </div>
                                    </div>


                                    <div class="row">
                                        <div class="col-lg-4 col-md-4">
                                            <input type="checkbox" id="vehicle1" name="vehicle1" value="Bike" class="check">
                                            <label for="vehicle1"> Keep me Logged In</label>
                                        </div>
                                        <div class="col-lg-4 col-md-4">
                                            <input class="btn btn_12" type="submit" value="Login" />
                                        </div>
                                        @*<div class="col-lg-4 col-md-4">
                                            <h4 class="refer">Refer our Terms & Conditions</h4>
                                        </div>*@
                                    </div>



                                    <div class="row">
                                        <div class="col-lg-12 col-md-12">
                                            <a class="samat fa-home" asp-action="MemberHome" asp-controller="Home">Member Home</a>


                                            <p class="samat">www.samatabhratrumandal.com</p>
                                        </div>

                                    </div>
                                </form>
                            </div>
                        </div>

                    </div>

                    <div class="navigation-top">

                    </div>




                </div>

            </div>
        </div>
    </section>


    <!-- jquery plugins here-->
    <!-- jquery -->
    <script src="~/js/jquery-1.12.1.min.js"></script>
    <!-- popper js -->
    <script src="~/js/popper.min.js"></script>
    <!-- bootstrap js -->
    <script src="~/js/bootstrap.min.js"></script>
    <!-- easing js -->
    <script src="~/js/jquery.magnific-popup.js"></script>
    <!-- swiper js -->
    <script src="~/js/swiper.min.js"></script>
    <!-- swiper js -->
    <script src="~/js/masonry.pkgd.js"></script>
    <!-- particles js -->
    <script src="~/js/owl.carousel.min.js"></script>
    <script src="~/js/jquery.nice-select.min.js"></script>
    <!-- swiper js -->
    <script src="~/js/slick.min.js"></script>
    <script src="~/js/jquery.counterup.min.js"></script>
    <script src="~/js/waypoints.min.js"></script>
    <!-- custom js -->
    <script src="~/js/custom.js"></script>
</body>

</html>
