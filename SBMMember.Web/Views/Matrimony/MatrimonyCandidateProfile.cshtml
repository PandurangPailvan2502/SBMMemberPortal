                                    <li>@Model.CandidateProfile.fullName</li>
                                    <li>Age:@Model.CandidateProfile.age yrs</li>
                                    <li>ऊंची : @Model.CandidateProfile.heightInFeet'@Model.CandidateProfile.heightInInches"</li>
                                    <li>Candidate Id:@Model.CandidateProfile.applicationId</li>
                                </ul>
                            </section>
