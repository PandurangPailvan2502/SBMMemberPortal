                                <i class="ti-edit"></i>
                                <img src="@Model.CandidateProfile.profileImage" style="margin:7px;height:110px;width:123px; border-radius:10px">
                            </div>
                            <section class="data-content1" style="background:#7c0053; width: calc(100% - 150px); padding:20px; color:#ffffff">
