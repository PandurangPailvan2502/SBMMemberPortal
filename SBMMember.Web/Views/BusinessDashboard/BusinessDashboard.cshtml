@model BannerAndMarqueeViewModel
@{
    ViewData["Title"] = "BusinessDashboard";
    Layout = "~/Views/Shared/_LayoutMemberDashboard.cshtml";
}
@*<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>*@

<div class="container-fluid" style="margin-top:30px">
    <div class="row">
        <div class="col-lg-12">
            <div class="scroll" style="background-color:darkred">
                <p> Welcome to Buisness Directory</p>
            </div>
        </div>
    </div>
</div>
<div class="seperate"></div>
<div class="container ">
    <div class="row">
        <div class="col-lg-12" style="text-align:center">
            <!------------icon dashbaord row-1--------------->
            <div class="container">
                <div class="row ">
                    <div class="col-xs-6 r1c1 dashboard" id="businessReg" style="cursor:pointer">
                        <a asp-action="RegisterBusiness" asp-controller="BusinessDashboard">   <img src="~/img/online-registration.png" class="dashboard-icon"></a>
                        <h3 style="color:#000000"><span>Register your business</span></h3>
                    </div>
                    <div class="col-xs-6 r1c1 dashboard" style="cursor:pointer" id="BusinessDirectory">
                        <a asp-action="BusinessDirectory" asp-controller="BusinessDashboard"> <img src="~/img/business-search.png" class="dashboard-icon"></a>
                        <h3 style="color:#000000"><span>Search directory</span></h3>
                    </div>
                </div>
            </div>
            <!------------End of icon dashbaord row-1--------------->
        </div>
    </div>
</div>
<div class="seperate"></div>
@await Html.PartialAsync("_PartialSlider")

<!--------------end of dashboard-------------->
