
@{
    ViewData["Title"] = "MemberContactInfo";
    Layout = "~/Views/Shared/_LayoutMemberForm.cshtml";
}
<input name="tab" id="tab2" type="radio" checked />
<section class="tab-content">
    <h2> मुळगाव व संपर्क</h2>
    <div class="section-top-border">
        <div class="row">
            <div class="col-lg-10 col-md-10">
                <form action="#">
                    <div class="input-group-icon mt-3">
                        <input type="text" name="nativePlace" placeholder="Native Place" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Native Place'"
                               required class="single-input">
                    </div>

                    <div class="input-group-icon mt-10">
                        <input type="text" name="nativePlaceTaluka" placeholder="Native Place Taluka" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Native Place Taluka'"
                               required class="single-input">
                    </div>

                    <div class="input-group-icon mt-10">
                        <input type="text" name="nativePlaceDistrict" placeholder="Native Place District" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Native Place District'"
                               required class="single-input">
                    </div>


                    <div class="input-group-icon mt-10">
                        <input type="text" name="mobile" placeholder="Personal Mobile 1" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Personal Mobile 1'"
                               required class="single-input">
                    </div>

                    <div class="input-group-icon mt-10">
                        <input type="text" name="mobile" placeholder="Personal Mobile 2" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Personal Mobile 2'"
                               required class="single-input">
                    </div>

                    <div class="input-group-icon mt-10">
                        <input type="email" name="email" placeholder="Email" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Email'"
                               required class="single-input">
                    </div>
                    <div class="input-group-icon mt-10">
                        <input type="text" name="Fname" placeholder="Ref. Full Name" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Ref. Full Name'"
                               required class="single-input">
                    </div>
                    <div class="input-group-icon mt-10">
                        <input type="text" name="address" placeholder="Reference / Relative full address" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Reference / Relative full address'"
                               required class="single-input">
                    </div>
                    <div class="input-group-icon mt-10">
                        <input type="text" name="mobile1" placeholder="Reference Mobile Number 1" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Reference Mobile Number 1'"
                               required class="single-input">
                    </div>
                    <div class="input-group-icon mt-10">
                        <input type="text" name="mobile2" placeholder="Reference Mobile Number 1" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Reference Mobile Number 2'"
                               required class="single-input">
                    </div>
                    <div class="mt-10 center">
                        <button type="submit" class="genric-btn primary-border">Save & Continue..</button>
                    </div>
                </form>
            </div>

        </div>
    </div>
</section>

