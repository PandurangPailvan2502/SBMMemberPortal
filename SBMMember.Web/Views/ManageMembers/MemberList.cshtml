@model MemberListViewModel
@{
    ViewData["Title"] = "MemberList";
    Layout = "~/Views/Shared/_LayoutAdminDashboard.cshtml";
}


<script>
    $(document).ready(function () {
        $("#BusinessForm").hide();
        $('#tblJobPost').DataTable();
        $("#btnAddNew").click(function () {
            $("#BusinessForm").show();
            $(this).hide();
        });
        $("#btnCancel").click(function () {
            $("#BusinessForm").hide();
            $("#btnAddNew").show();
        });
    });
</script>

<section class="blog_area single-post-area section_padding">
    <div class="container">

        <div class="row">
            <div class="col-lg-12">
                <div class="scroll">
                    <p> Approved Member List</p>
                </div>
                <div>
                </div>
            </div>
        </div>




    </div>


    <div class="container">

        <div class="row">
            <div class="col-lg-12">
                <table id="tblJobPost" class="display" style="width:100%">
                    <thead>
                        <tr>
                            <th>Membership Id</th>
                            <th>Prefix</th>
                            <th>FirstName</th>
                            <th>MiddleName</th>
                            <th>LastName</th>
                            <th>Mobile</th>
                            <th>City</th>
                           <th>Form Status</th>
                            <th>Operations</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (Model.memberList != null)
                        {
                            foreach (var member in Model.memberList)
                            {
                        <tr>
                            <td>@member.MemberShippId</td>
                            <td>@member.Prefix</td>
                            <td>@member.FirstName</td>
                            <td>@member.MiddleName</td>
                            <td>@member.LastName</td>
                            <td>@member.MobileNumber</td>
                            <td>@member.City</td>
                            <td>@member.FormStatus</td>
                            <td>
                                <a title="View Profile" href="@Url.Action("VerifyMemberProfile","ManageMembers",new { memberId = member.MemberId})"><i class="ti-search"></i> View Profile</a>
                                <a title="DeActivate" href="@Url.Action("DeActivateMemberProfile","ManageMembers",new { memberId = member.MemberId})"><i class="ti-control-pause"></i> DeActivate</a>
                            </td>
                        </tr>
                            }

                        }
                    </tbody>

                </table>
            </div>
        </div>
    </div>

</section>

