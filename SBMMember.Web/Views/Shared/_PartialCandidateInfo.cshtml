
@model CandidateSearchResponse

@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@
<div class="seperate"></div>

<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <div class="container search_result">
                <div class="row">
                    <div class="col-xs-3 search_pic">
                        <div text-align:center">
                            <img src="@Model.profileImage" class="pic_img">
                        </div>
                    </div>
                    <div class="col-xs-9 pic_content">
                        <div class="container">
                            <div class="row">
                                <div class="col-xs-10 left-box"><lable>@Model.fullName</lable></div>
                            </div>
                            <div class="row">
                                <div class="col-xs-10 left-box"><lable>वय : @Model.age</lable></div>
                            </div>
                          
                            <div class="row">
                                <div class="col-xs-5 left-box"><lable>शिक्षण : @Model.education</lable></div>
                                <div class="col-xs-7 left-box" style="float:right"><a href="@Url.ActionLink("MatrimonyCandidateProfile","Matrimony",new{UserIdId = Model.userId })" class="view-btn">View</a></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>