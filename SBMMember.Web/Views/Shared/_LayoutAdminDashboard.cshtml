</head>

<body>
    <!--------------profile name top bar---------------->
  <div class="container">
    <div class="row">
        <div class="col-lg-12">
            <!------------icon dashbaord row-1--------------->
            <div class="container">
                <div class="row ">
                    <div class="col-xs-4 logo-box">
                        <img src="~/img/sbm logo-1.png" class="logo">
                    </div>
                    <div class="col-xs-6 title-box">
                        <h2>समता भ्रातृ मंडळ</h2>
                        <h4>(पिंपरी चिंचवड, पुणे)</h4>
                    </div>
                    <div class="col-xs-2 toolbar">
                        <h4 class="ic"><i class="ti-lock"></i> <a asp-action="Login" asp-controller="Login" class="sp">Login</a></h4>
                    </div>

                </div>
            </div>
            <!------------End of icon dashbaord row-1--------------->
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="scroll">
                <p><marquee>Welcome to SBM Member portal.</marquee></p>
            </div>
            <div>
            </div>
        </div>
    </div>

</div>
    
    <!----------------------end of scroll bar---------------->
    <div class="seperate"></div>
    @RenderBody()

    <div class="seperate"></div>

    <div class="container ">
        <div class="row">
            <div class="col-lg-12" style="text-align:center">

                <div class="container">
                    <table class="table" cellspacing="5" cellpadding="5">
                        <tbody>
                            <tr>
                                <td>Notification</td>
                                <td></td>
                                <td>New member</td>
                                <td></td>
                                <td>Recent Jobs</td>
                            </tr>
                            <tr>
                                <td style="background-color:#db044c;color:#ffffff;font-size:16px;">1</td>
                                <td style="width:5px"></td>
                                <td style="background-color:#ffda08;color:#ffffff;font-size:16px;">25</td>
                                <td style="width:5px"></td>
                                <td style="background-color:#be0303;color:#ffffff;font-size:16px;">3</td>
                            </tr>
                        </tbody>
                    </table>
                </div>

            </div>
        </div>
    </div>



    <div class="seperate"></div>
    <hr>

    <div class="container ">
        <div class="row">
            <div class="col-lg-12" style="text-align:center">
                <!------------icon dashbaord row-2--------------->
                <div class="container">
                    <table class="table" cellspacing="5" cellpadding="5">
                        <tbody>
                            <tr>

                                <td><img src="~/img/videomeet.png" class="table-button"></td>
                                <td style="width:20px"></td>
                                <td><img src="~/img/about us-1.png" class="table-button"></td>

                            </tr>
                        </tbody>
                    </table>
                </div>
                <!------------End of icon dashbaord row-2--------------->
            </div>
        </div>
    </div>
    <!-- jquery plugins here-->
    <!-- jquery -->
    <script src="~/js/jquery-1.12.1.min.js"></script>
    <!-- popper js -->
    <script src="~/js/popper.min.js"></script>
    <!-- bootstrap js -->
    <script src="~/js/bootstrap.min.js"></script>
    <!-- easing js -->
    <script src="~/js/jquery.magnific-popup.js"></script>
    <!-- swiper js -->
    <script src="~/js/swiper.min.js"></script>
    <!-- swiper js -->
    <script src="~/js/masonry.pkgd.js"></script>
    <!-- particles js -->
    <script src="~/js/owl.carousel.min.js"></script>
    <script src="~/js/jquery.nice-select.min.js"></script>
    <!-- swiper js -->
    <script src="~/js/slick.min.js"></script>
    <script src="~/js/jquery.counterup.min.js"></script>
    <script src="~/js/waypoints.min.js"></script>
    <!-- custom js -->
    <script src="~/js/custom.js"></script>
